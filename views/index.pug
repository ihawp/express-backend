extends layout

block content
    header(class='flex flex-col items-center text-center mx-3')
        img(src='/images/w.webp' alt='Warren Chemerika outside wearing sunglasses.'
            class='rounded mb-1 hero-image-hover transition-all' width='225' height='225')
        h1(class='mb-half c-2 title') Warren Chemerika
        h2(class='bg-5 px-half py-1 br-4 font-weight-600 subtitle mb-half') Web Developer @ Servoweb
        p(class='max-w-400 flex items-center paratitle')
            svg(
                class="min-w-1-25 mr-half"
                viewBox="0 0 21 21"
                height="20"
                width="20"
                xmlns:xlink="http://www.w3.org/1999/xlink"
                xmlns="http://www.w3.org/2000/svg"
            )
                image(
                    height="20"
                    width="20"
                    y="0"
                    x="0"
                    xlink:href="data:image/png;base64,"
                )
            | Certified by Saskatchewan Polytechnic


    section(class='flex flex-col gap-1 mx-2')
        h2(aria-label="Portfolio section: my work and personal projects" hidden) Portfolio
        each item in content
            div(
                class='project p-1 pb-2 pointer flex flex-col items-center b-only-top-2 bs-solid bc-5' id=`project-${item.id}`)
                div(class='flex justify-between w-full items-center gap-1')
                    div(class='flex gap-1 items-center')
                        svg(
                            class="min-w-1-25 opacity-10 display-sm"
                            viewBox="0 0 20 20"
                            height="20"
                            width="20"
                            xmlns:xlink="http://www.w3.org/1999/xlink"
                            xmlns="http://www.w3.org/2000/svg"
                        )
                            image(
                                height="20"
                                width="20"
                                y="0"
                                x="0"
                                xlink:href="data:image/png;base64,"
                            )
                        h3(class='font-weight-500 font-size-1-5')= item.title
                    div
                        ul(class='flex gap-1 flex-grow-1')
                            if (item.github !== '')
                                li
                                    a(href=`${item.github}` class='bg-5 bg-h-6 p-half rounded flex items-center justify-center')
                                        svg(aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="white" viewBox="0 0 25 25")
                                            path(fill-rule="evenodd" d="M12.006 2a9.847 9.847 0 0 0-6.484 2.44 10.32 10.32 0 0 0-3.393 6.17 10.48 10.48 0 0 0 1.317 6.955 10.045 10.045 0 0 0 5.4 4.418c.504.095.683-.223.683-.494 0-.245-.01-1.052-.014-1.908-2.78.62-3.366-1.21-3.366-1.21a2.711 2.711 0 0 0-1.11-1.5c-.907-.637.07-.621.07-.621.317.044.62.163.885.346.266.183.487.426.647.71.135.253.318.476.538.655a2.079 2.079 0 0 0 2.37.196c.045-.52.27-1.006.635-1.37-2.219-.259-4.554-1.138-4.554-5.07a4.022 4.022 0 0 1 1.031-2.75 3.77 3.77 0 0 1 .096-2.713s.839-.275 2.749 1.05a9.26 9.26 0 0 1 5.004 0c1.906-1.325 2.74-1.05 2.74-1.05.37.858.406 1.828.101 2.713a4.017 4.017 0 0 1 1.029 2.75c0 3.939-2.339 4.805-4.564 5.058a2.471 2.471 0 0 1 .679 1.897c0 1.372-.012 2.477-.012 2.814 0 .272.18.592.687.492a10.05 10.05 0 0 0 5.388-4.421 10.473 10.473 0 0 0 1.313-6.948 10.32 10.32 0 0 0-3.39-6.165A9.847 9.847 0 0 0 12.007 2Z" clip-rule="evenodd")
                            if (item.url !== '')
                                li
                                    a(href=`${item.url}` class='bg-5 bg-h-6 p-half rounded flex items-center justify-center')
                                        svg(aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="none" viewBox="0 0 25 25")
                                            path(stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.213 9.787a3.391 3.391 0 0 0-4.795 0l-3.425 3.426a3.39 3.39 0 0 0 4.795 4.794l.321-.304m-.321-4.49a3.39 3.39 0 0 0 4.795 0l3.424-3.426a3.39 3.39 0 0 0-4.794-4.795l-1.028.961")
                div(class='display pb-1 mt-1')
                    each text in item.description
                        p(class=`mt-1`)= text
                    div(class='flex gap-1 flex-grow-1 justify-center flex-wrap mt-2')
                        each logo in item.languages
                            -
                            svg(xmlns="http://www.w3.org/2000/svg" fill="var(--6)" width="55" height="55" viewBox="0 0 800 600")
                                path(d=logo)
    section
        h2 Where did I study?
        div
            div
                img(src='' alt='')
                div
                    h3 Title of School
                    h4 Certificate
                    p Years Attended
            div
                p content



    <!-- scripts -->
    script(src='javascripts/pug-projects.js')